{
  "name": "qwik-city-movies",
  "description": "Qwik City Movies",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "-------BUILD-------": "",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/netlify-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "-------DEV-------": "",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "-------LINT-------": "",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "fmt.lint": "eslint \"src/**/*.ts*\" --fix",
    "fmt.tsc": "tsc --noEmit",
    "-------PREVIEW-------": "",
    "preview": "qwik build preview && vite preview --open",
    "serve": "netlify dev",
    "start": "vite --open --mode ssr",
    "-------UTILS-------": "",
    "qwik": "qwik",
    "find-deadcode": "ts-prune"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.12.1",
    "@builder.io/qwik-city": "0.0.118",
    "@netlify/vite-plugin-netlify-edge": "1.1.1",
    "@types/eslint": "8.4.10",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "autoprefixer": "10.4.13",
    "eslint": "8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-qwik": "0.12.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "node-fetch": "3.2.10",
    "postcss": "^8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.2",
    "ts-prune": "^0.10.3",
    "typescript": "4.8.4",
    "vite": "3.2.3",
    "vite-tsconfig-paths": "3.5.2"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "daisyui": "^2.38.1",
    "zod": "^3.19.1"
  }
}
